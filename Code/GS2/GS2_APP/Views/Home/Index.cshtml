@model dynamic

<h2 class="mb-4">Dashboard</h2>

<div class="row">
    <div class="col-md-4">
        <div class="card text-white bg-primary mb-3">
            <div class="card-header">Rotas em Andamento</div>
            <div class="card-body">
                <h5 class="card-title">@ViewBag.RotasAndamento.ToString()</h5>
            </div>
        </div>
    </div>
    <div class="col-md-4">
        <div class="card text-white bg-info mb-3">
            <div class="card-header">Cursos Concluídos</div>
            <div class="card-body">
                <h5 class="card-title">@ViewBag.CursosConcuidos.ToString()</h5>
            </div>
        </div>
    </div>
</div>



<h2 class="mb-4">KPIs</h2>

<div class="row">
    <div class="col-md-4">
        <div class="card text-white bg-secondary mb-3">
            <div class="card-header">Progresso</div>
            <div class="card-body">
                <h5 class="card-title">@ViewBag.ProgressoMedio.ToString("F2")%</h5> <!-- Exibe o progresso médio -->
            </div>
        </div>
    </div>
    <div class="col-md-4">
        <div class="card text-white bg-info mb-3">
            <div class="card-header">Tempo</div>
            <div class="card-body">
                <h5 class="card-title">@ViewBag.Tempo.ToString()</h5>
            </div>
        </div>
    </div>
</div>
<h2 class="mb-4">Últimos Acessos</h2>
<div class="row">
</div>
